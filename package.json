{
    "name": "ccash-vscode",
    "displayName": "C$",
    "description": "C$ language support for Visual Studio Code",
    "version": "0.0.10",
    "publisher": "Blond11516",
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Blond11516/ccash-vscode.git"
    },
    "homepage": "https://github.com/Blond11516/ccash-vscode",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ccash",
                "aliases": [
                    "C$",
                    "ccash"
                ],
                "extensions": [
                    ".ccash"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ccash",
                "scopeName": "source.ccash",
                "path": "./syntaxes/ccash.tmLanguage.json"
            }
        ]
    }
}
